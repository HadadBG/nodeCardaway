  <div id="modal1" class="modal">
    <div class="modal-content">
      <form id="formPostal" enctype="multipart/form-data">
        <div class="row">
          <div class="input-field col s12">
            <select id="selector">
              <option value="Amor">
                Amor
              </option>
              <option value="Fechas Festivas">
                Fechas Festivas
              </option>
              <option value="Paisajes">
                Paisajes
              </option>
              <option value="Comida">
                Comida
              </option>
              <option value="Vintage">
                Vintage
              </option>
              <option value="Otros">
                Otros
              </option>
            </select>
            <label>
              Categoria
            </label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <textarea id="textarea1" class="materialize-textarea"></textarea>
            <label for="textarea1">
              Palabras Clave
            </label>
          </div>
        </div>
        <div class="row">
          <div class="file-field input-field col s12">
            <div class="btn">
              <span>
                Seleccionar Archivo
              </span>
              <input type="file" name="uploadedFile" id="uploadedFile" />
            </div>
            <div class="file-path-wrapper">
              <input class="file-path validate" type="text" />
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col s12">
            <button type="submit" name="action" class="btn">
              Agregar Postal
            </button>
          </div>
        </div>
        <div class="row">
          <div class="col s12">
            <a class="right">
              *Formatos Aceptados:(gif,jpg,png)
            </a>
          </div>
        </div>
      </form>
    </div>
  </div>
  <div id="test1" class="col s12">
    <div class="container">
      <div class="tabla-header">
        <p class="titulo">
          Usuarios
        </p>

        <div class="div-search">
          <input
            id="buscadorU"
            placeholder="Busqueda"
            type="text"
            class="search"
          />
          <button>
            <i class="material-icons">
              search
            </i>
          </button>
        </div>
      </div>
      <div class="row">
        <div class="col s12 center">
          <table class="highlight" id="mitabla">
            <thead>
              <tr>
                <th class="headFoto">
                  Foto
                </th>
                <th class="headNombre">
                  Nombre
                </th>
                <th class="headCorreo">
                  Correo
                </th>
                <th class="headNac">
                  Nacimiento
                </th>
                <th>
                  Acción
                </th>
              </tr>
            </thead>
            <tbody id="cargadorU">
            </tbody>
          </table>
        </div>
      </div>
      <div class="buttons">
        <button id="leftArrow">
          <i class="material-icons arrow">
            navigate_before
          </i>
        </button>
        <div class="noPage">
          <i class="texto" id="noPage">
            1
          </i>
        </div>
        <button id="rightArrow">
          <i class="material-icons arrow">
            navigate_next
          </i>
        </button>
      </div>
    </div>
  </div>

  <div id="test2" class="col s12">
    <div class="container">
      <div class="tabla-header">
        <p class="titulo">
          Postales
        </p>

        <div class="addButton modal-trigger" data-target="modal1">
          <i class="add material-icons">
            add
          </i>
        </div>
        <div class="div-search">
          <input
            id="buscadorP"
            placeholder="Busqueda"
            type="text"
            class="search"
          />
          <button>
            <i class="material-icons">
              search
            </i>
          </button>
        </div>
      </div>
      <div class="row">
        <div class="col s12 center">
          <table class="highlight" id="mitabla">
            <thead>
              <tr>
                <th class="headPostal">
                  Postal
                </th>
                <th class="headCategoria">
                  Categoria
                </th>
                <th class="headDescripcion">
                  Fecha de Subida
                </th>
                <th>
                  Acción
                </th>
              </tr>
            </thead>
            <tbody id="cargadorP">
            </tbody>
          </table>
        </div>
      </div>
      <div class="buttons">
        <button id="leftArrowP">
          <i class="material-icons arrow">
            navigate_before
          </i>
        </button>
        <div class="noPage">
          <i class="texto" id="noPageP">
            1
          </i>
        </div>
        <button id="rightArrowP">
          <i class="material-icons arrow">
            navigate_next
          </i>
        </button>
      </div>
    </div>
  </div>

